<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8" />
  </startup>
  <rdcman>
    <!-- Size options for client size and remote desktop size. Only the first ten are used. -->
    <displaySizes>
      <add size="800 x 600"/>
      <add size="1024 x 768"/>
      <add size="1280 x 1024"/>
      <add size="1366 x 768"/>
      <add size="1440 x 900"/>
      <add size="1600 x 1200"/>
      <add size="1920 x 1200"/>
    </displaySizes>
    <programUpdate>
      <versionPath></versionPath>
      <updateUrl></updateUrl>
    </programUpdate>
    <logging>
      <enabled></enabled>
      <path></path>
      <maximumNumberOfFiles></maximumNumberOfFiles>
    </logging>
    <warningThresholds>
      <connect></connect>
    </warningThresholds>
  </rdcman>
</configuration>
